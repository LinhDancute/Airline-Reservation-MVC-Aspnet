                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1">
                    <label class="form-check-label" style="color: rgb(0, 95, 110);" for="inlineRadio1">Tiền</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2">
                    <label class="form-check-label" style="color: rgb(0, 95, 110);" for="inlineRadio2">Dặm bay</label>
                </div>
                <div class="form-check form-check-inline">
                    <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3">
                    <label class="form-check-label" style="color: rgb(0, 95, 110);" for="inlineRadio3">Tiền & Dặm</label>
                </div>
                <hr />
                <span class="title-recent-searches" style="cursor: pointer; font-size: 1.2em;" onclick="openBoxSearchHistory()">
                    <img src="~/images/clock.png" style="width: 23px; height: auto;" /> TRA CỨU GẦN ĐÂY
                </span>
                <div class="recent-search-wrap active" id="box-search-history">
                    <div class="content-recent-searches">
                        <div class="box-recent-searches">
                            <div class="container-sm">
                                <div class="my-2 card">
                                    <div class="col">
                                        <div class="row">
                                            <p class="title-history text-center">
                                                <span>ROUND TRIP</span> - Economy
                                            </p>
                                        </div>
                                        <div class="row justify-content-md-center">
                                            <div class="flight-recent-searches">
                                                <div class="row bg-warning">
                                                    <div class="col">
                                                        <div class="item-flight text-center">
                                                            <span class="icon-flight">
                                                                <img src="~/images/icon-flight-recent-search.png" style="width: 18px; height: auto;">
                                                            </span>(Tp. Hồ Chí Minh (SGN), Việt Nam)
                                                        </div>
                                                    </div>
                                                    <div class="col">
                                                        <div class="item-flight text-center">
                                                            <span class="icon-flight">
                                                                <img src="~/images/icon-flight-recent-search.png" style="width: 18px; height: auto;">
                                                            </span>(Hà Nội (HAN), Việt Nam)
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a href="#" class="btn-recent-searches" data-device="desktop" data-class="Economy" data-flight-type="roundtrip"
                           data-flight-count="1" data-adult="1" data-child="0" data-infant="0" data-flight-from-1="Tp. Hồ Chí Minh (SGN), Việt Nam"
                           data-flight-to-1="Hà Nội (HAN), Việt Nam" data-flight-depart-code-1="SGN" data-flight-arrival-code-1="HAN"><img src="" alt="" width="7" height="auto"></a>
                    </div>
                </div>
                <script type="text/javascript">
                    function openBoxSearchHistory() {
                        var boxSearchHistory = document.getElementById('box-search-history');
                        if (boxSearchHistory.style.display === 'none' || boxSearchHistory.style.display === '') {
                            boxSearchHistory.style.display = 'block';
                        } else {
                            boxSearchHistory.style.display = 'none';
                        }
                    }
                </script>
                <!--Location-->
                <div class="container-sm">
                    <div class="my-2 card">
                        <div class="card-body">
                            <h5 class="card-title">Địa điểm</h5>
                            <div class="row">
                                <div class="col-sm">
                                    <div class="mb-2">
                                        <label id="origin-label" for="origin-input" class="form-label">Điểm đi</label>
                                        <div class="input-group"></div>
                                    </div>
                                </div>
                                <div class="col-sm">
                                    <div class="mb-2">
                                        <label id="destination-label" for="destination-input" class="form-label">Điểm đến</label>
                                        <div class="input-group"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-pane fade" id="manage-booking" role="tabpanel" aria-labelledby="manage-booking-tab">...</div>
            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">...</div>
            <div class="tab-pane fade" id="flight-looking" role="tabpanel" aria-labelledby="flight-looking-tab">...</div>
        </div>
    </div>
    @RenderBody()
    
    <!-- Footer-->
    <footer class="py-5 bg-dark">
        <div class="container px-4 px-lg-5"><p class="m-0 text-center text-white">Copyright &copy; LinhdanAirlines 2024</p></div>
    </footer>
